package miniproject.service;

import java.util.List;

import miniproject.model.BarangModel;


public interface IBarangService {

	public BarangModel save(BarangDto barang);
	public BarangModel findById(int id);
	public List<BarangModel> findAll();
	public void deleteById(int id);
	public BarangModel update (int id, BarangDto barang);
}

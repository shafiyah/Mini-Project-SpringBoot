public class BarangConverter implements IBarangConverter {
	
	public BarangConverter() {
		
	}

	@Override
	public BarangDto createFrom(BarangModel barangEntity) {
		BarangDto barangDto= new BarangDto();
		barangDto.setDeskripsiBarang(deskripsiBarang);
		barangDto.setIdBarang(idBarang);
		barangDto.setNamaBarang(namaBarang);
		barangDto.setStokBarang(stokBarang);	
		return barangDto;
	}


package miniproject.service;

import java.util.List;

import miniproject.model.BarangModel;
import miniproject.model.PemesanModel;

public interface IBarangService {

	public BarangModel save(BarangModel barang);
	public BarangModel findById(int id);
	public List<PemesanModel> findAll();
	public void deleteById(int id);
	public PemesanModel update (int id, PemesanModel pemesan);
}

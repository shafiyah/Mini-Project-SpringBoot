package miniproject.endpoint.response;

import java.io.Serializable;

public class DetailPemesananResponse implements Serializable {
	private static final long serialVersionUID = 1L;
	
	private int idDetailpemesanan;
	private int idPemesanan;
	private String namaBarang;
	private int jumlahBarang;
	
	public DetailPemesananResponse() {
	}

	public DetailPemesananResponse(int idDetailpemesanan, int idPemesanan, String namaBarang, int jumlahBarang) {
		super();
		this.idDetailpemesanan = idDetailpemesanan;
		this.idPemesanan = idPemesanan;
		this.namaBarang = namaBarang;
		this.jumlahBarang = jumlahBarang;
	}

	public int getIdDetailpemesanan() {
		return idDetailpemesanan;
	}

	public void setIdDetailpemesanan(int idDetailpemesanan) {
		this.idDetailpemesanan = idDetailpemesanan;
	}

	public int getIdPemesanan() {
		return idPemesanan;
	}

	public void setIdPemesanan(int idPemesanan) {
		this.idPemesanan = idPemesanan;
	}

	public String getNamaBarang() {
		return namaBarang;
	}

	public void setidBarang(String namaBarang) {
		this.namaBarang = namaBarang;
	}

	public int getJumlahBarang() {
		return jumlahBarang;
	}

	public void setJumlahBarang(int jumlahBarang) {
		this.jumlahBarang = jumlahBarang;
	}
}

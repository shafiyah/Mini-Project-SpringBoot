package miniproject.repository.dto;

import java.io.Serializable;

import miniproject.repository.dto.BarangDto;
import miniproject.repository.dto.PemesananDto;



public class DetailPemesananDto implements Serializable {
	
	private static final long serialVersionUID = 1L;
	private int idDetailpemesanan;
	private PemesananDto pemesanan;
	private BarangDto barang;
	private int jumlah_barang;
	
	public DetailPemesananDto() {
	}

	public DetailPemesananDto(int idDetailpemesanan, PemesananDto pemesanan, BarangDto barang, int jumlah_barang) {
		super();
		this.pemesanan = pemesanan;
		this.barang = barang;
		this.jumlah_barang = jumlah_barang;
	}

	public int getIdDetailpemesanan() {
		return idDetailpemesanan;
	}

	public void setIdDetailpemesanan(int idDetailpemesanan) {
		this.idDetailpemesanan = idDetailpemesanan;
	}

	public PemesananDto getPemesanan() {
		return pemesanan;
	}

	public void setPemesananDto(PemesananDto pemesanan) {
		this.pemesanan = pemesanan;
	}

	public BarangDto getBarang() {
		return barang;
	}

	public void setBarang(BarangDto barang) {
		this.barang = barang;
	}

	public int getJumlah_barang() {
		return jumlah_barang;
	}

	public void setJumlah_barang(int jumlah_barang) {
		this.jumlah_barang = jumlah_barang;
	}
}
